extends layout

append head
    link(rel="stylesheet" href="/stylesheets/my-list.css")

block content
        div.titleDiv
            h2.myList MY LISTS
            div.buttonDiv
                button(type="submit").buttonCSS
                    a(href='/my-lists/new') New List
        div.listContainer
            each list in lists
                div.backgroundContain
                    div.listCard#list.id
                        a(href=`/my-lists/${list.id}`)
                            if list.name === 'Watched'
                                img(src="https://media.istockphoto.com/photos/woman-watching-online-tv-in-the-night-picture-id1010556014?k=20&m=1010556014&s=612x612&w=0&h=zuSouPIJ2ZC7nbSQRaI-RS3kD6h0KLWnZQU0VFT1KfQ=" alt="Movie Image Thumbnail")

                            if list.name === 'To Watch'
                                img(src="https://images.wsj.net/im-427883?width=1280&size=1" alt="Movie Image Thumbnail")
                            if list.name === 'My Fave Romance Comedies'
                                img(src="data:image/jpeg;base64," alt="Movie Image Thumbnail")
                            if list.name === 'Classic Films'
                                img(src="data:image/jpeg;base64," alt="Movie Image Thumbnail")
                            else if list.name !== 'Watched' && list.name !== 'To Watch' && list.name !== 'My Fave Romance Comedies'
                                img(src="https://cdn3.vectorstock.com/i/1000x1000/65/02/coming-soon-sale-poster-vector-1666502.jpg" alt="Movie Image Thumbnail")
                            //-     p= list.name
                            p= list.name
                            